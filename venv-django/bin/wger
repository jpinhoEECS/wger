#!/home/jason/Desktop/EECS481/Homeworks/HW6/wger/venv-django/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'wger','console_scripts','wger'
__requires__ = 'wger'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('wger', 'console_scripts', 'wger')()
    )
